<% set_meta_tags title: 'Liked Rubocops',
                 description: 'All of the Rubocop configs that you have liked',
                 og: { title: 'Liked Rubocops' } %>

<section class="py-12 bg-white sm:py-16 lg:py-20">
  <div class="container">
    <header>
      <h1 class="text-3xl font-extrabold tracking-tight text-gray-900 sm:text-4xl">
        Liked Rubocops
      </h1>
      <p class="mt-4 text-xl text-gray-500">
        All the cops that you have liked
      </p>
    </header>
    <main class="grid gap-16 pt-12 mt-6 border-t-2 border-gray-100 lg:grid-cols-2 lg:gap-x-6 lg:gap-y-12">
      <%= render partial: 'posts/post', collection: @posts %>
    </main>
  </div>
</section>
